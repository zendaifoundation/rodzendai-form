    "version": "0.2.0",
    "configurations": [
        {
            "name": "rodzendai form [chrome][debug] [sandbox]",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "deviceId": "chrome",
            "args": [
                "lib/main.dart",
                "--dart-define-from-file",
                ".env_sandbox",
                "--web-port=5173"
            ],
            "toolArgs": [
                "--disable-service-auth-codes",
                "--no-dds"
            ]
        },
        {
            "name": "rodzendai form [chrome][profile] [sandbox]",
